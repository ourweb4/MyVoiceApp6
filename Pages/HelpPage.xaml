<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyVoiceApp6.Pages.HelpPage"
             Title="Help">
    <ContentPage.Padding>
        <OnPlatform x:TypeArguments="Thickness">
            <OnPlatform.Platforms>
                <On Platform="iOS" Value="0, 20, 0, 0" />
                <On Platform="Android" Value="0, 0, 0, 0" />
                <On Platform="UWP" Value="0, 0, 0, 0" />
            </OnPlatform.Platforms>
        </OnPlatform>
    </ContentPage.Padding>

    <StackLayout
        HorizontalOptions="Center"
        VerticalOptions="Center" >

        <Label Text="Our Voice App Pro"
               FontSize="Header" />
        <Label  x:Name="lblver"  HorizontalOptions="CenterAndExpand"/>
        <Label Text="774-778-3847"  HorizontalOptions="CenterAndExpand"/>
        <Label Text="(c)2023, Ourweb.net" HorizontalOptions="CenterAndExpand" />

    </StackLayout>


</ContentPage>